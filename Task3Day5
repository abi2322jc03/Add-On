package day4java;// file write String

import java.io.*;

public class Task3Day5 {



	public static void main(String[] args) {

		try {

			FileWriter fw=new FileWriter("C:\\Users\\casstudent\\sample.txt");

			fw.write("hi this is abishek bca 2");

			fw.close();

			System.out.println("the information is added in sample file");

		}catch(IOException e) {

			System.out.println("file not found");

		}

		



	}



}
